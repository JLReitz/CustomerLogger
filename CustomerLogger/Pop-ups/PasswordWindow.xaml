<Window x:Class="CustomerLogger.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CustomerLogger"
        mc:Ignorable="d"
        Title="Admin Password" Height="130.07" Width="349.301" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>
        <Button x:Name="OkButton" Content="OK" Margin="0,0,8,6" Click="OkButton_Click" RenderTransformOrigin="2.253,0.85" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75"/>
        <PasswordBox x:Name="PasswordBox" PasswordChar="*" MaxLength="32" HorizontalAlignment="Left" Margin="10,24,0,0" VerticalAlignment="Top" Width="313" Height="24" KeyUp="PasswordBox_KeyUp"/>
        <Label x:Name="PwLabel" Content="Enter Admin Password:" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Top" Width="328"/>
    </Grid>
</Window>
